<?xml version="1.0" encoding="utf-8"?>
<ScrollView  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main5Activity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="800dp">


        <TextView
            android:id="@+id/textView6"
            android:layout_width="60dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="180dp"
            android:layout_marginLeft="180dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="171dp"
            android:layout_marginRight="171dp"
            android:text="Part 2"
            android:textColor="#4CAF50"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="265dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView6"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="78dp"
            android:layout_marginLeft="78dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="66dp"
            android:layout_marginRight="66dp"
            android:text="Enter the voltage that you have:"
            android:textColor="#009688"
            android:textSize="18sp" />

        <EditText
            android:id="@+id/volta1"
            android:layout_width="115dp"
            android:layout_height="48dp"
            android:layout_below="@+id/textView11"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="34dp"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="24dp"
            android:ems="10"
            android:hint="Voltage slack"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/volta2"
            android:layout_width="115dp"
            android:layout_height="48dp"
            android:layout_below="@+id/volta1"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="34dp"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="24dp"
            android:ems="10"
            android:hint="Voltage bar 2"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/volta3"
            android:layout_width="115dp"
            android:layout_height="48dp"
            android:layout_below="@+id/volta2"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="34dp"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="24dp"
            android:ems="10"
            android:hint="Voltage bar 3"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/volta4"
            android:layout_width="115dp"
            android:layout_height="48dp"
            android:layout_below="@+id/volta3"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="34dp"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="24dp"
            android:ems="10"
            android:hint="Voltage bar 4"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/volta5"
            android:layout_width="115dp"
            android:layout_height="48dp"
            android:layout_below="@+id/volta4"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="34dp"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="24dp"
            android:ems="10"
            android:hint="Voltage bar 5"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/volta6"
            android:layout_width="115dp"
            android:layout_height="48dp"
            android:layout_below="@+id/volta5"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="34dp"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="24dp"
            android:ems="10"
            android:hint="Voltage bar 6"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/iterNum"
            android:layout_width="115dp"
            android:layout_height="48dp"
            android:layout_below="@+id/textView12"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_marginStart="34dp"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="24dp"
            android:ems="10"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="245dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/volta6"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="87dp"
            android:layout_marginLeft="87dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="79dp"
            android:layout_marginRight="79dp"
            android:text="Enter the number of iterations:"
            android:textColor="#009688"
            android:textSize="18sp" />

        <Button
            android:id="@+id/nextbutton"
            android:layout_width="81dp"
            android:layout_height="38dp"
            android:layout_below="@+id/textView12"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="162dp"
            android:layout_marginLeft="162dp"
            android:layout_marginTop="98dp"
            android:layout_marginEnd="161dp"
            android:layout_marginRight="161dp"
            android:background="#00BCD4"
            android:text="Next" />

    </RelativeLayout>


</ScrollView >

