        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:ems="10"
        android:hint="Voltage bar 5"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
